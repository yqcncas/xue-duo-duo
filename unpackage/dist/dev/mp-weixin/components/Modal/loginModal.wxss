          transform: translate(-50%, -50%);
  z-index: 9999;
  visibility: hidden;
  background: #fff;
  border-radius: 36rpx;
  -webkit-transition: all 0.3s ease-in-out;
  transition: all 0.3s ease-in-out;
}
.modal-dialog-show {
  visibility: visible;
}
